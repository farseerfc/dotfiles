Host fs2
    HostName fs2.osipp.osaka-u.ac.jp
    IdentityFile ~/.ssh/id_rsa
    User yang
    ControlMaster auto
    ControlPath ~/.ssh/socket-fs2-%C
    ControlPersist yes
    ServerAliveInterval 10
    KexAlgorithms +diffie-hellman-group1-sha1

